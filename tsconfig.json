{
	"compilerOptions": {
		"module": "commonjs",
		"noImplicitAny": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"sourceMap": true,
		"strictNullChecks": true,
		"preserveConstEnums": true,
		"lib": ["ES2017","DOM","ES2015", "ES7", "ES2018", "ES2019", "ES2022", "ES2021", "ES2020"],
		"target": "es5",
	},
	"include": ["src/**/*"],
  	"exclude": ["node_modules", "**/*.spec.ts", "**/*.hbs"]
}